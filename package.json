{
	"name": "manual-sorting",
	"version": "2.5.1",
	"description": "An Obsidian plugin that adds manual drag&drop sorting to the file explorer.",
	"author": "kh4f <kh4f.dev@gmail.com>",
	"license": "MIT",
	"repository": "https://github.com/kh4f/manual-sorting",
	"bugs": "https://github.com/kh4f/manual-sorting/issues",
	"homepage": "https://github.com/kh4f/manual-sorting#readme",
	"keywords": [
		"obsidian",
		"obsidian-md",
		"obsidian-plugin",
		"obsidian-sorting",
		"custom-sorting",
		"manual-sorting",
		"obsidian-manual-sorting",
		"drag-and-drop"
	],
	"engines": {
		"node": "^24.10.0"
	},
	"packageManager": "pnpm@10.18.2",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"build:dev": "tsdown",
		"build:watch": "tsdown --watch src",
		"build:prod": "tsdown --prod",
		"lint": "eslint",
		"lint:fix": "eslint --fix",
		"bench": "vitest bench --run",
		"release": "relion",
		"release:github": "relion -p github",
		"prepare": "simple-git-hooks"
	},
	"devDependencies": {
		"@eslint/js": "^9.37.0",
		"@stylistic/eslint-plugin": "^5.4.0",
		"@types/node": "^24.7.2",
		"@types/sortablejs": "^1.15.8",
		"eslint": "^9.37.0",
		"globals": "^16.4.0",
		"happy-dom": "^20.0.0",
		"i18next": "^25.6.0",
		"jiti": "^2.6.1",
		"lint-staged": "^16.2.4",
		"obsidian": "^1.10.0",
		"obsidian-typings": "^4.58.0",
		"relion": "^0.23.0",
		"simple-git-hooks": "^2.13.1",
		"tsdown": "^0.15.6",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.0",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"monkey-around": "^3.0.0",
		"sortablejs": "^1.15.6"
	},
	"simple-git-hooks": {
		"pre-commit": "pnpm lint-staged"
	},
	"lint-staged": {
		"*.ts": "pnpm lint:fix"
	}
}
